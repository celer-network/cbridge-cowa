{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DepositMsg",
  "type": "object",
  "required": [
    "dst_chid",
    "mint_acnt",
    "nonce"
  ],
  "properties": {
    "dst_chid": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "mint_acnt": {
      "type": "string"
    },
    "nonce": {
      "description": "ETH address Hex string without 0x prefix",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
